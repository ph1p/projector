{
  "name": "projector",
  "version": "0.3.0",
  "private": true,
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "register-service-worker": "^1.0.0",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.5.17",
    "vue-color": "^2.4.6",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1",
    "vue-snotify": "^3.2.1",
    "vuejs-datepicker": "^1.5.2",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "^3.0.0-rc.10",
    "@vue/cli-service": "^3.0.0-rc.10",
    "babel-plugin-transform-decorators": "^6.24.1",
    "conventional-changelog-cli": "^2.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.14.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
